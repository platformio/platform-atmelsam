{
  "build": {
    "arduino": {
        "ldscript": "flash.ld"
    },
    "core": "arduino",
    "cpu": "cortex-m3",
    "extra_flags": "-D__SAM3X8E__ -DARDUINO_SAM_DUE",
    "f_cpu": "84000000L",
    "hwids": [
      [
        "0x2341",
        "0x003E"
      ],
      [
        "0x2341",
        "0x003D"
      ],
      [
        "0x2A03",
        "0x003D"
      ]
    ],
    "mcu": "at91sam3x8e",
    "usb_product": "Arduino Due",
    "variant": "arduino_due_x",
    "zephyr": {
       "variant": "arduino_due"
    }
  },
  "connectivity": [
    "can"
  ],
  "debug": {
    "jlink_device": "ATSAM3X8E",
