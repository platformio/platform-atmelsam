{
    "build": {
      "arduino": {
          "ldscript": "flash_with_bootloader.ld"
      },
      "core": "facts-engineering",
      "cpu": "cortex-m4",
      "extra_flags": [
        "-DADAFRUIT_GRAND_CENTRAL_M4",
        "-D__SAMD51P20A__",
        "-D__SAMD51__",
        "-D__FPU_PRESENT",
        "-DARM_MATH_CM4",
        "-DUSB_VID=0x1354",
        "-DUSB_PID=0x4002",
        "-DUSBCON",
        "-DUSB_CONFIG_POWER=100",
        "-DUSB_MANUFACTURER=FACTS Engineering LLC",
        "-DUSB_PRODUCT=P1AM-200"
      ],
      "f_cpu": "120000000L",
      "hwids": [
        [
          "0x1354",
          "0x4002"
        ],
        [
          "0x1354",
          "0x4003"
        ],
        [
          "0x1354",
          "0x4004"
        ]
      ],
      "mcu": "samd51p20a",
      "usb_product": "P1AM-200",
      "variant": "p1am200"
    },
    "debug": {
        "jlink_device": "ATSAMD51P20",
        "openocd_chipname": "at91samd51p20",
        "openocd_target": "atsame5x",
        "svd_path": "ATSAMD51P20A.svd"
    },
    "frameworks": [
      "arduino"
    ],
    "name": "P1AM-200",
    "upload": {
      "disable_flushing": true,
      "maximum_ram_size": 262144,
      "maximum_size": 1048576,
      "native_usb": true,
      "offset_address": "0x4000",
      "protocol": "sam-ba",
      "protocols": [
        "sam-ba",
        "jlink",
        "atmel-ice"
      ],

      "require_upload_port": true,
      "use_1200bps_touch": true,
      "wait_for_upload_port": true
    },
    "url": "https://www.automationdirect.com/adc/shopping/catalog/programmable_controllers/open_source_controllers_(arduino-compatible)",
    "vendor": "FACTS Engineering, LLC"
  }