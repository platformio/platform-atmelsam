{
  "build": {
    "core": "adafruit",
    "system": "samd",
    "extra_flags": "-D__SAMD21G18A__ -DARDUINO_ARCH_SAMD -DARDUINO_SAMD_ZERO -DDADAFRUIT_METRO_M0_EXPRESS -lsamd21_qtouch_gcc",
    "cpu": "cortex-m0plus",
    "f_cpu": "48000000L",
    "hwids": [
      [
        "0x239A",
        "0x8013"
      ],
      [
        "0x239A",
        "0x0013"
      ]
    ],
    "ldscript": "flash_with_bootloader.ld",
    "mcu": "samd21g18a",
    "usb_product": "Metro M0 Expres",
    "variant": "metro_m0"
  },
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-s", "$PACKAGE_DIR/scripts",
            "-f", "interface/cmsis-dap.cfg",
            "-c", "set CHIPNAME at91samd21g18; set ENDIAN little; set telnet_port 0",
            "-f", "target/at91samdXX.cfg"
          ]
        },
        "load_cmd": "preload"
      },
      "blackmagic": {
        "hwids": [
          [
            "0x1d50",
            "0x6018"
          ]
        ],
        "require_debug_port": true
      }
    }
  },
  "frameworks": [
    "arduino"
  ],
  "name": "Adafruit Metro M0 Expresss",
  "upload": {
    "disable_flushing": true,
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "native_usb": true,
    "protocol": "sam-ba",
    "require_upload_port": true,
    "use_1200bps_touch": true,
    "wait_for_upload_port": true
  },
  "url": "https://www.adafruit.com/product/3505",
  "vendor": "Adafruit"
}
