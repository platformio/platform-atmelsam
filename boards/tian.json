{
  "build": {
    "core": "samd",
    "cpu": "cortex-m0plus",
    "extra_flags": "-DARDUINO_SAMD_ZERO -DARDUINO_ARCH_SAMD -D__SAMD21G18A__",
    "f_cpu": "48000000L",
    "hwids": [
      [
        "0x2A03",
        "0x8052"
      ],
      [
        "0x10C4",
        "0xEA70"
      ]
    ],
    "ldscript": "flash_with_bootloader.ld",
    "mcu": "samd21g18a",
    "usb_product": "Arduino Tian",
    "variant": "arduino_mzero"
  },
  "connectivity": [
    "ethernet"
  ],
  "debug": {
    "jlink_device": "ATSAMD21G18",
