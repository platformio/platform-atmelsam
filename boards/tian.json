    "openocd_target": "at91samdXX",
    "svd_path": "ATSAMD21G18A.svd"
  },
  "frameworks": [
    "arduino",
    "cmsis"
  ],
  "name": "Arduino Tian",
  "upload": {
    "disable_flushing": true,
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "offset_address": "0x4000",
    "protocol": "stk500v2",
